# Mesh related options
[Mesh]
   [./Generation]
      dimension = '3'
      element_type = 'HEX8'
      n_elems_x = '10'
      n_elems_y = '10'
      n_elems_z = '2'
[]

[Variables]
   [./Velocity]
      names = 'Ux Uy Uz'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
   [../Pressure]
      names = 'p'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
   [../Temperature]
      names = 'T'
      fe_family = 'LAGRANGE'
      order = 'FIRST'
[]

[Physics]
   enabled_physics = 'IncompressibleNavierStokes HeatTransfer'
[]
